{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    "source": "~/.dotfiles/fastfetch/ascii-spaceman.txt",
    "color": {
        "1": "cyan",
        "2": "blue",
        "3": "magenta"
    }
  },
  "display": {
    "separator": " ➜  "
  },
  "modules": [
    {
      "type": "custom",
      "format": "┌─────────────────────  System Information  ────────────────────┐"
    },
    "break",
    {
      "type": "cpu",
      "key": "    ",
      "keyColor": "blue"
    },
    {
      "type": "board",
      "key": "    ",
      "keyColor": "blue"
    },
    {
      "type": "gpu",
      "key": "  󰛇  ",
      "keyColor": "yellow"
    },
    {
      "type": "gpu",
      "format": "{3}",
      "key": "    ",
      "keyColor": "magenta"
    },
    {
      "type": "memory",
      "key": "    ",
      "keyColor": "green"
    },
    // {
    //   "type": "display",
    //   "key": "  󰍹  ",
    //   "keyColor": "red",
    //   "compactType": "original"
    // },
    "break",
    {
      "type": "os",
      "key": "    ",
      "keyColor": "blue"
    },
    {
      "type": "kernel",
      "key": "    ",
      "keyColor": "yellow"
    },
    {
      "type": "shell",
      "key": "    ",
      "keyColor": "magenta"
    },
    {
      "type": "wm",
      "key": "    ",
      "keyColor": "green"
    },
    // {
    //   "type": "terminal",
    //   "key": "    ",
    //   "keyColor": "red"
    // },
    {
      "type": "packages",
      "key": "    ",
      "keyColor": "black"
    },
    "break",
    {
      "type": "custom",
      "format": "└───────────────────────────────────────────────────────────────┘"
    }
  ]
}
